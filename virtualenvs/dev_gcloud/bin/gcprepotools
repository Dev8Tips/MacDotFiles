#!/Users/Neo/.dotfiles/virtualenvs/dev_gcloud/bin/python2.7
# EASY-INSTALL-ENTRY-SCRIPT: 'gcp-python-repo-tools==0.0.1','console_scripts','gcprepotools'
__requires__ = 'gcp-python-repo-tools==0.0.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('gcp-python-repo-tools==0.0.1', 'console_scripts', 'gcprepotools')()
    )
